  "dependencies": {
    "aws-sdk": "^2.100.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1"
  }
}
